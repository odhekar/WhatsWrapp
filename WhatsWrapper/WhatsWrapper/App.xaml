<common:BootStrapper x:Class="WhatsWrapper.App"
                     xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                     xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                     xmlns:common="using:Template10.Common" xmlns:local="using:WhatsWrapper" >
    <common:BootStrapper.Resources>
        <Style x:Key="HeaderTextBox" TargetType="TextBlock">
            <Setter Property="FontSize" Value="20" />
            <Setter Property="Foreground" Value="{ThemeResource SystemControlForegroundAccentBrush}" />
        </Style>
    </common:BootStrapper.Resources>

</common:BootStrapper>
